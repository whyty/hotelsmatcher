<% var temp, i, l; %>
<div class="book details floating hidden">
    <ul class="hotel info column">
        <li class="header">
            Paramount Zone Park Central New York
        </li>
        <li class="radio trips pin">
            <input type="hidden" name="pinned" value="false" />
            <span class="label">hotel pinned</span>
            <div class="check"></div>
        </li>
        <li class="photos">
            <button class="previous"></button>
            <div class="images">
                <img src="http://lorempixel.com/158/82/city" />
            </div>
            <button class="next"></button>
        </li>
        <li class="price"><span>Price </span><span class="currency"> $ </span><span class="value">345</span></li>
        <li class="book action"><button type="button" class="button book-action" title="Book for this hotel">Book now</button></li>
    </ul>
</div>
<ul class="headers column">
    <li class="header">
        Hotels
    </li>
    <li class="pin"></li>
    <li class="price"><span>Price</span></li>
    <li class="stars"><span>Stars</span></li>
    <li class="tripadvisor-reviews"><span>TripAdvisor Reviews</span></li>
    <li class="distance"><span>Distance from area</span></li>
    <li class="photos"><span>Photos</span></li>
    <li class="facilities category title"><span>Facilities</span></li>
    <li class="wifi option checkbox unchecked"><div class="check"></div><span>Wifi</span></li>
    <li class="parking option checkbox unchecked"><div class="check"></div><span>Parking</span></li>
    <li class="airport-shuttle option checkbox unchecked"><div class="check"></div><span>Airport Shuttle</span></li>
    <li class="fitness-center option checkbox unchecked"><div class="check"></div><span>Fitness Center</span></li>
    <li class="spa option checkbox unchecked"><div class="check"></div><span>Spa</span></li>
    <li class="see-all facilities"><button type="button" class="see-all facilities">&gt; see all facilities</button></li>
    <li class="theme category title"><span>Hotel theme</span></li>
    <li class="family option checkbox unchecked"><div class="check"></div><span>Family</span></li>
    <li class="business option checkbox unchecked"><div class="check"></div><span>Business</span></li>
    <li class="romance option checkbox unchecked"><div class="check"></div><span>Romance</span></li>
    <li class="budget option checkbox unchecked"><div class="check"></div><span>Budget</span></li>
    <li class="see-all"><button type="button" class="see-all theme">&gt; see all themes</button></li>
    <li class="book action"></li>
</ul>
<div class="pinned-container hidden">
</div>
<% if(obj) _.each(obj,function(item,key,arr) { %>
<% temp = item.attributes %>
<div class="hotel info column fixed">
    <ul>
        <li class="header">
            <%= temp.name %>
        </li>
        <li class="radio trips pin">
            <input type="hidden" name="pinned" value="false" />
            <span class="label">hotel pinned</span>
            <div class="check"></div>
        </li>
        <li class="price"><span> $ </span><span class="value">345</span></li>
        <li class="stars">
            <div class="rating" title="<%=temp.hotelRating %>">
                <%
                l = temp.hotelRating;
                for(i = 0; i < 5; i++) {
                    if(l > i) {
                    %><span class="active"></span><%
                    } else {
                    %><span></span><%
                    }
                } %>
            </div>
        </li>
        <li class="tripadvisor-reviews">
            <div class="rating" title="<%=temp.tripAdvisorRating %>">
                <%
                l = temp.tripAdvisorRating;
                for(i = 0; i < 5; i++) {
                    if(l > i) {
                    %><span class="active"></span><%
                    } else {
                    %><span></span><%
                    }
                } %>
            </div>
        </li>
        <li class="distance"><span><%=temp.proximityDistance %> km</span></li>
        <li class="photos">
            <button class="previous"></button>
            <div class="images">
                <img src="http://lorempixel.com/125/115/city" />
            </div>
            <button class="next"></button>
        </li>
        <li class="facilities category title">Facilities</li>
        <li class="wifi option"><span class="checkbox unchecked">true</span></li>
        <li class="parking option"><span class="checkbox unchecked">false</span></li>
        <li class="airport-shuttle option"><span class="checkbox unchecked">true</span></li>
        <li class="fitness-center option"><span class="checkbox unchecked">true</span></li>
        <li class="spa option"><span class="checkbox unchecked">true</span></li>
        <li class="see-all"></li>
        <li class="facilities category title">Hotel theme</li>
        <li class="family option"><span class="checkbox unchecked">true</span></li>
        <li class="business option"><span class="checkbox checked">true</span></li>
        <li class="romance option"><span class="checkbox unchecked">true</span></li>
        <li class="budget option"><span class="checkbox unchecked">true</span></li>
        <li class="see-all"></li>
        <li class="book action"><button type="button" class="button book-action" title="Book for this hotel">Book now</button></li>
    </ul>
</div>
<% }); %>
